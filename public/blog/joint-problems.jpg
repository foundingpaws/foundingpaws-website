<svg width="400" height="300" viewBox="0 0 400 300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f0f8f0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e8f5e8;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#bg)"/>
  
  <!-- Dog silhouette with joint highlights -->
  <g transform="translate(200, 150)">
    <!-- Body outline -->
    <ellipse cx="0" cy="0" rx="60" ry="40" fill="none" stroke="#2c5f2d" stroke-width="3"/>
    
    <!-- Head -->
    <circle cx="0" cy="-50" r="25" fill="none" stroke="#2c5f2d" stroke-width="3"/>
    
    <!-- Legs with joint circles -->
    <line x1="-30" y1="20" x2="-20" y2="50" stroke="#2c5f2d" stroke-width="3"/>
    <line x1="-20" y1="50" x2="-15" y2="80" stroke="#2c5f2d" stroke-width="3"/>
    <line x1="30" y1="20" x2="20" y2="50" stroke="#2c5f2d" stroke-width="3"/>
    <line x1="20" y1="50" x2="15" y2="80" stroke="#2c5f2d" stroke-width="3"/>
    
    <!-- Joint circles (highlighted) -->
    <circle cx="-25" cy="35" r="8" fill="#d4af37" stroke="#2c5f2d" stroke-width="2"/>
    <circle cx="25" cy="35" r="8" fill="#d4af37" stroke="#2c5f2d" stroke-width="2"/>
    <circle cx="-17" cy="65" r="6" fill="#d4af37" stroke="#2c5f2d" stroke-width="2"/>
    <circle cx="17" cy="65" r="6" fill="#d4af37" stroke="#2c5f2d" stroke-width="2"/>
  </g>
  
  <!-- Medical icons -->
  <g transform="translate(50, 50)">
    <circle cx="0" cy="0" r="15" fill="#d4af37" opacity="0.2"/>
    <path d="M -8 -8 L 8 8 M 8 -8 L -8 8" stroke="#2c5f2d" stroke-width="2"/>
  </g>
  
  <g transform="translate(350, 80)">
    <circle cx="0" cy="0" r="15" fill="#d4af37" opacity="0.2"/>
    <path d="M -8 -8 L 8 8 M 8 -8 L -8 8" stroke="#2c5f2d" stroke-width="2"/>
  </g>
  
  <!-- Text overlay -->
  <text x="200" y="250" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#2c5f2d" font-weight="bold">
    Gelenkprobleme erkennen
  </text>
</svg>
