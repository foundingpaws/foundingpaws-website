<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f0f8f0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e8f5e8;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="dog" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2c2c2c;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a1a1a;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="400" fill="url(#bg)"/>
  
  <!-- Border Collie -->
  <g transform="translate(200, 200)">
    <!-- Body -->
    <ellipse cx="0" cy="20" rx="50" ry="30" fill="url(#dog)"/>
    
    <!-- Head -->
    <ellipse cx="0" cy="-25" rx="28" ry="32" fill="url(#dog)"/>
    
    <!-- Ears -->
    <ellipse cx="-18" cy="-45" rx="6" ry="15" fill="url(#dog)" transform="rotate(-10)"/>
    <ellipse cx="18" cy="-45" rx="6" ry="15" fill="url(#dog)" transform="rotate(10)"/>
    
    <!-- Eyes -->
    <circle cx="-8" cy="-30" r="2.5" fill="#2c1810"/>
    <circle cx="8" cy="-30" r="2.5" fill="#2c1810"/>
    
    <!-- Nose -->
    <ellipse cx="0" cy="-22" rx="2.5" ry="2" fill="#2c1810"/>
    
    <!-- Mouth -->
    <path d="M -5 -18 Q 0 -14 5 -18" stroke="#2c1810" stroke-width="1.5" fill="none"/>
    
    <!-- Legs -->
    <rect x="-16" y="45" width="6" height="20" fill="url(#dog)"/>
    <rect x="-3" y="45" width="6" height="20" fill="url(#dog)"/>
    <rect x="3" y="45" width="6" height="20" fill="url(#dog)"/>
    <rect x="16" y="45" width="6" height="20" fill="url(#dog)"/>
    
    <!-- Tail -->
    <ellipse cx="35" cy="0" rx="10" ry="5" fill="url(#dog)" transform="rotate(20)"/>
    
    <!-- Paws -->
    <circle cx="-13" cy="65" r="4" fill="#2c1810"/>
    <circle cx="0" cy="65" r="4" fill="#2c1810"/>
    <circle cx="6" cy="65" r="4" fill="#2c1810"/>
    <circle cx="19" cy="65" r="4" fill="#2c1810"/>
  </g>
  
  <!-- Focused expression lines -->
  <g stroke="#2c2c2c" stroke-width="2" fill="none" opacity="0.6">
    <path d="M 70 80 Q 80 70 90 80"/>
    <path d="M 310 80 Q 320 70 330 80"/>
  </g>
  
  <!-- Text overlay -->
  <text x="200" y="350" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" fill="#2c5f2d" font-weight="bold">
    Lisa & Luna
  </text>
  <text x="200" y="370" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#2c5f2d">
    Border Collie, 5 Jahre
  </text>
</svg>
